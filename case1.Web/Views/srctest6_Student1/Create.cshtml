@model case1.Web.Models.srctest6.Student1Model

@{
    ViewBag.Title = "Create";
    Layout  = "~/Views/Shared/_Layout.cshtml";
}

<div class="m_title_box">
    <ol>
        <li data-id="srctest6">
            <a href="#">srctest6</a><i class="icon-arrowright"></i>
        </li>
        <li data-id="Student1">
            <a href="#">Student1</a><i class="icon-arrowright"></i>
        </li>
        <li data-id="Create">Create</li>
    </ol>
</div>
<div class="panel panel-default">
    <div class="panel-body">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
            @Html.HiddenFor(model => model.Id)
            @Html.HiddenFor(model => model.OwnerId)
			<div class="row">
				<div class="col-md-6">
					<div class="form-group">
						<label for="Name"  class="col-md-4 from-title" >Name</label>
						<div class="col-md-8 from-cont">
							@Html.TextBoxFor(model => model.Name, new { @class = "form-control" })
							@Html.ValidationMessageFor(model => model.Name)
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group">
						<label for="Description"  class="col-md-4 from-title" >Description</label>
						<div class="col-md-8 from-cont">
							@Html.TextBoxFor(model => model.Description, new { @class = "form-control" })
							@Html.ValidationMessageFor(model => model.Description)
						</div>
					</div>
				</div>
			</div><div class="row">
				<div class="col-md-12">
					<div class="form-group">
						<label class="col-md-2 from-title" >StudentName</label>
						<div class="col-md-10 from-cont">
							@Html.TextAreaFor(model => model.StudentName, new { @class = "form-control" })
							@Html.ValidationMessageFor(model => model.StudentName)
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-6">
					<div class="form-group">
						<label for="StudentNUM"  class="col-md-4 from-title" >StudentNUM</label>
						<div class="col-md-8 from-cont">
							@Html.TextBoxFor(model => model.StudentNUM, new { @class = "form-control" })
							@Html.ValidationMessageFor(model => model.StudentNUM)
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group">
						<label for="user_id"  class="col-md-4 from-title" >user_id</label>
						<div class="col-md-8 from-cont">
							@Html.TextBoxFor(model => model.user_id, new { @class = "form-control" })
							@Html.ValidationMessageFor(model => model.user_id)
						</div>
					</div>
				</div>
			</div>
            <div class="width_10 text-center m_left">
                <input class="m_btn m_back" type="submit" value="Save" />
                @Html.ActionLink("Cancel", "Index", null, new {@class="m_btn m_back"})
            </div>
        }
    </div>
</div>

