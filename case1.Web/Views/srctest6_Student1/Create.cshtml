<script src="@Url.Content("~/Scripts/ckeditor_full/ckeditor.js")"> </script>
    <script>
        $(document)
            .ready(function() {
				CKEDITOR.replace('StudentName', { customConfig: '/Scripts/ckeditor_full/config.js' });
